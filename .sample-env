# pylint: skip-file
# .env file

# port application is served on
PORT=8000

# set to 'production' or 'development'
# https://nodejs.dev/learn/nodejs-the-difference-between-development-and-production
NODE_ENV="development"

# path to python executable; default ''
# Note: if a relative path is specified, it must be relative to the project root folder.
# e.g. if using a venv virtual environment (https://docs.python.org/3/library/venv.html#module-venv)
#      in the project root folder, use './venvd/Scripts/'
PYTHON_PATH='./venvd/Scripts/'

# python executable; default 'python3'
# Note: if running on windows, full filename including extension is required.
PYTHON_EXE='python.exe'

# path to app folder; default '/app'
# Note: if a relative path is specified, it must be relative to the project root folder.
# e.g. the default location is the project root folder, so use './'
APP_PATH='./'

# Google Drive API credentials
CREDS=""

# name of credentials file; default 'creds.json'
CREDS_FILE="creds.json"

# path to credentials file; default
# Note: if a relative path is specified, it must be relative to the project root folder.
# e.g. the default location is the project root folder, so use './'
CREDS_PATH="./"

# RapidAPI credentials
RAPID_CREDS=""

# name of RapidAPI credentials file; default 'rapid_creds.json'
RAPID_CREDS_FILE="rapid_creds.json"

# path to RapidAPI credentials file; default
# Note: if a relative path is specified, it must be relative to the project root folder.
# e.g. the default location is the project root folder, so use './'
RAPID_CREDS_PATH="./"

# name of Google Sheets spreadsheet
SPREADSHEET_NAME="AnalaStock"

